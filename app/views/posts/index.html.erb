<h1>All Posts</h1>
<%= link_to "New post", new_post_path %>


<% @posts.each do |post| %>
	<hr>
	<h2><%= post.title %></h2>
	<%= link_to "Show", post_path(post.id) %>
	<%= link_to "Edit", edit_post_path(post) %>
	<%= link_to "Delete", post_path(post.id), method: :delete, data: { confirm: 'Are you sure?' } %>
<% end %>
